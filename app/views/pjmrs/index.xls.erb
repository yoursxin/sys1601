      <Cell><Data ss:Type="Number"><%=pjmr.pjmc.zclx if pjmr.pjmc != nil %></Data></Cell>
      <Cell><Data ss:Type="Number"><%=pjmr.pjmc.ssje if pjmr.pjmc != nil %></Data></Cell>
      <Cell><Data ss:Type="String"><%=pjmr.pjmc.khjlmc if pjmr.pjmc != nil %></Data></Cell>
      <Cell><Data ss:Type="String"><%=pjmr.pjmc.dabh if pjmr.pjmc != nil %></Data></Cell>
