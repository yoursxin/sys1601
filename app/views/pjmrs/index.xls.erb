       <Cell><Data ss:Type="String"><%=pjmr.ph%></Data></Cell>
      <Cell><Data ss:Type="String"><%=pjmr.txlx%></Data></Cell>
      <Cell><Data ss:Type="String"><%=pjmr.pjlx%></Data></Cell>
