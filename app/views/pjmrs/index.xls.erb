      <Cell><Data ss:Type="String"><%=pjmr.pjmc.khmc if pjmr.pjmc != nil %></Data></Cell>
      <Cell><Data ss:Type="String"><%=pjmr.pjmc.zmrq if pjmr.pjmc != nil %></Data></Cell>
      <Cell><Data ss:Type="Number"><%=pjmr.pjmc.txlx if pjmr.pjmc != nil %></Data></Cell>
      <Cell><Data ss:Type="Number"><%=pjmr.pjmc.jxts if pjmr.pjmc != nil %></Data></Cell>
      <Cell><Data ss:Type="String"><%=pjmr.pjmc.jxdqrq if pjmr.pjmc != nil %></Data></Cell>
