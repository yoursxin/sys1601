<h4>票据买入查询</h4>
<%=form_tag pjmrs_path, method: :get, class: 'form-horizotal' do%>
<%=render partial: 'pjmrFindCons'%>	
<%=submit_tag "查询", class: "btn btn-large btn-primary" %>

<% end %>
<hr>
<%=form_tag delete_multiple_pjmrs_path, method: :delete,name: 'pjmrListForm' do %>

<%=render partial: 'pjmrList', object: @pjmrs%>

<%= will_paginate %>
<%= submit_tag "删除", data: {confirm: "确认删除吗？"}%>
<% end %>

<h4>批量导入</h4>
<%=form_tag import_pjmrs_path, multipart: true do%>
  <%=file_field_tag :file%>
  <%=submit_tag "导入"%>
<% end%>
