<%=render partial: 'zjtzList', object: @zjtzs %>
<%=submit_tag "结清审核通过", class: "btn  btn-primary",data: {confirm: "确认进行结清审核通过吗？"}, name: 'cjsh_btn'%>
<%=submit_tag "结清申请退回", class: "btn btn-danger",data: {confirm: "确认进行结清审核退回吗？"}	, name: 'cjsqth_btn'%>
<% end %>